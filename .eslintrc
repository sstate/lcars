{
  "env": {
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true,
    "nomen": true
  },
  "globals": {
    "jest": true,
    "module": false,
    "require": false,
    "after": true,
    "afterEach": true,
    "beforeEach": true,
    "describe": true,
    "expect": true,
    "it": true,
    "jasmine": true,
    "runs": true,
    "spyOn": true,
    "spyOnEvent": true,
    "waitsFor": true,
    "loadFixtures": true,
    "React": true,
  },
  "rules": {
    "quotes": [2, "single"],
    "no-trailing-spaces": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "eol-last": 0,
    "space-infix-ops": 0,
    "no-trailing-spaces": 0,
    "new-cap": 0
  }
}